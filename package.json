{
  "name": "neko2.0",
  "version": "2.0.0",
  "description": "Bot Discord Neko - Version 2.0 avec Docker et PostgreSQL",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon main.js",
    "prisma:generate": "prisma generate --schema=./config/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./config/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./config/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./config/prisma/schema.prisma",
    "docker:build": "cd config/docker && docker-compose build",
    "docker:up": "cd config/docker && docker-compose up -d",
    "docker:down": "cd config/docker && docker-compose down",
    "docker:logs": "cd config/docker && docker-compose logs -f bot",
    "docker:restart": "cd config/docker && docker-compose restart bot",
    "docker:dev": "cd config/docker && docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev-down": "cd config/docker && docker-compose -f docker-compose.dev.yml down"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "postgresql",
    "prisma",
    "docker"
  ],
  "author": "Ma'",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.41",
    "@prisma/client": "^5.9.0",
    "canvas": "^2.11.2",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.10",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "glob": "^10.3.10",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "prisma": "^5.9.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
